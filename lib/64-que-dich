<script src="https://tuvi.kabala.vn/js/kabala-text.js"></script>

<!-- JavaScript cho chức năng tìm kiếm -->
<script>
function highlightText() {
    var input = document.getElementById("searchBox").value.toLowerCase();
    var table = document.getElementById("queDichTable");
    var cells = table.getElementsByTagName("td");
    var found = false;

    // Đặt lại màu nền của các ô
    for (var i = 0; i < cells.length; i++) {
        cells[i].style.backgroundColor = "";
    }

    // Duyệt qua tất cả các ô để tìm nội dung khớp
    for (var i = 0; i < cells.length; i++) {
        var cellText = cells[i].textContent || cells[i].innerText;
        if (cellText.toLowerCase().includes(input) && input !== "") {
            cells[i].style.backgroundColor = "yellow"; // Đánh dấu ô khớp
            if (!found) {
                cells[i].scrollIntoView({ behavior: "smooth", block: "center" });
                found = true; // Chỉ cuộn đến lần khớp đầu tiên
            }
        }
    }
}
</script>


